##########################################################
#
# RESOURCES
#
##########################################################
##########################################################
### ###########################################
### webPTM:webPTMfiles

:webPTMfiles rdf:type owl:Class ;

      rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :haswebPTMdata ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :haswebPTMinfo ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :haswebPTMdescript ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :haswebPTMcomment ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger                      ] ;

      knora-base:resourceIcon "webPTM.gif" ;

      rdfs:label "webPTMfiles"@de ,
                 "webPTMfiles"@fr ,
                 "webPTMfiles"@en ;

      rdfs:comment """The webPTM consists of the image files and the info json file"""@en ,

      knora-base:hasDefaultRestrictedViewPermission knora-base:UnknownUser ;

      knora-base:hasDefaultViewPermission knora-base:KnownUser ;

      knora-base:hasDefaultModifyPermission knora-base:ProjectMember ,
                                            knora-base:Owner .


### ###########################################
### webPTM:webPTMdata

:webPTMdata rdf:type owl:Class ;

          rdfs:subClassOf knora-base:StillImageRepresentation ,
                          [
                             rdf:type owl:Restriction ;
                             owl:onProperty :haswebPTMdata_comment ;
                             owl:minCardinality "0"^^xsd:nonNegativeInteger                          ] ;

          rdfs:label "webPTM tiff or png files"@en ;

          rdfs:comment """webPTM data with the images comprising the webPTM file (i.e. how it is tiled)"""@en ;

          knora-base:hasDefaultViewPermission knora-base:KnownUser ;

          knora-base:hasDefaultModifyPermission knora-base:ProjectMember ;

          knora-base:hasDefaultDeletePermission knora-base:Owner .



### ###########################################
### webPTM:webPTMinfo

:webPTMinfo rdf:type owl:Class ;

          rdfs:subClassOf knora-base:TextRepresentation ,
                          [
                             rdf:type owl:Restriction ;
                             owl:onProperty :haswebPTMinfo_comment ;
                             owl:minCardinality "0"^^xsd:nonNegativeInteger                          ] ;


          rdfs:label "webPTM json file"@en ;

          rdfs:comment """webPTM json file with internal information on the webPTM file (i.e. how it is tiled)"""@en ;

          knora-base:hasDefaultViewPermission knora-base:KnownUser ;

          knora-base:hasDefaultModifyPermission knora-base:ProjectMember ;

          knora-base:hasDefaultDeletePermission knora-base:Owner .

