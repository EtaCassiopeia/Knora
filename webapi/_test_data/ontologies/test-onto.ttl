# A trivial ontology, used only for testing Knora.

@base <http://www.knora.org/ontology/test> .
@prefix : <http://www.knora.org/ontology/test> .
<http://www.knora.org/ontology/test> rdf:type owl:Ontology .


<http://www.knora.org/ontology/test#treeList> rdf:type knora-base:ListNode ;

                                         knora-base:isRootNode "true"^^xsd:boolean ;

                                         rdfs:label "Tree list root"@en ;

                                         knora-base:hasSubListNode <http://www.knora.org/ontology/test#treeList01> ,
                                                                   <http://www.knora.org/ontology/test#treeList02> ,
                                                                   <http://www.knora.org/ontology/test#treeList03> .


<http://www.knora.org/ontology/test#treeList01> rdf:type knora-base:ListNode ;

                                         knora-base:listNodeName "Tree list node 01" ;

                                         knora-base:hasRootNode <http://www.knora.org/ontology/test#treeList> ;

                                         knora-base:listNodePosition "0"^^xsd:integer ;

                                         rdfs:label "Tree list node 01"@en .


<http://www.knora.org/ontology/test#treeList02> rdf:type knora-base:ListNode ;

                                         knora-base:listNodeName "Tree list node 02" ;

                                         knora-base:hasRootNode <http://www.knora.org/ontology/test#treeList> ;

                                         knora-base:listNodePosition "1"^^xsd:integer ;

                                         rdfs:label "Tree list node 02"@en .


<http://www.knora.org/ontology/test#treeList03> rdf:type knora-base:ListNode ;

                                         knora-base:listNodeName "Tree list node 03" ;

                                         knora-base:hasRootNode <http://www.knora.org/ontology/test#treeList> ;

                                         knora-base:listNodePosition "2"^^xsd:integer ;

                                         rdfs:label "Tree list node 03"@en ;

                                         knora-base:hasSubListNode <http://www.knora.org/ontology/test#treeList10> ,
                                                                   <http://www.knora.org/ontology/test#treeList11> .


<http://www.knora.org/ontology/test#treeList10> rdf:type knora-base:ListNode ;

                                         knora-base:listNodeName "Tree list node 10" ;

                                         knora-base:hasRootNode <http://www.knora.org/ontology/test#treeList> ;

                                         knora-base:listNodePosition "0"^^xsd:integer ;

                                         rdfs:label "Tree list node 10"@en .


<http://www.knora.org/ontology/test#treeList11> rdf:type knora-base:ListNode ;

                                         knora-base:listNodeName "Tree list node 11" ;

                                         knora-base:hasRootNode <http://www.knora.org/ontology/test#treeList> ;

                                         knora-base:listNodePosition "1"^^xsd:integer ;

                                         rdfs:label "Tree list node 11"@en .


:hasListItem rdf:type owl:ObjectProperty ;

       rdfs:subPropertyOf knora-base:hasValue ;

       rdfs:label "Listenelement"@de ,
                  "Elément de liste"@fr ,
                  "Elemento di lista"@it ,
                  "List element"@en ;

       knora-base:subjectClassConstraint :Thing ;

       knora-base:objectClassConstraint knora-base:FloatValue ;

       salsah-gui:guiOrder "0"^^xsd:integer ;

       salsah-gui:guiElement salsah-gui:List ;

       salsah-gui:guiAttribute "hlist=<http://www.knora.org/ontology/test#treeList>" ;

       knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

       knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:hasOtherThing rdf:type owl:ObjectProperty ;

               rdfs:subPropertyOf knora-base:hasLinkTo ;

               rdfs:label "Ein anderes Ding"@de ,
                          "Une autre chose"@fr ,
                          "Un'altra cosa"@it ,
                          "Another thing"@en ;

               knora-base:subjectClassConstraint :Thing ;

               knora-base:objectClassConstraint :Thing ;

               salsah-gui:guiOrder "1"^^xsd:integer ;

               salsah-gui:guiElement salsah-gui:Searchbox ;

               knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

               knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:hasOtherThingValue rdf:type owl:ObjectProperty ;

               rdfs:subPropertyOf knora-base:hasLinkToValue ;

               rdfs:label "Ein anderes Ding"@de ,
                          "Une autre chose"@fr ,
                          "Un'altra cosa"@it ,
                          "Another thing"@en ;

               knora-base:subjectClassConstraint :Thing ;

               knora-base:objectClassConstraint :LinkValue ;

               salsah-gui:guiOrder "1"^^xsd:integer ;

               salsah-gui:guiElement salsah-gui:Searchbox ;

               knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

               knora-base:hasDefaultModifyPermission knora-base:ProjectMember .


