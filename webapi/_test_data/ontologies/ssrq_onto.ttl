# PROPERTIES
#
##########################################################





:stdName rdf:type owl:ObjectProperty ;

       rdfs:subPropertyOf knora-base:hasValue, foaf:name ;

       rdfs:label "Name"@de ;

       knora-base:subjectClassConstraint :entityWithNames ;

       knora-base:objectClassConstraint knora-base:TextValue ;

       salsah-gui:guiOrder "1"^^xsd:integer ;

       salsah-gui:guiElement salsah-gui:SimpleText ;

       salsah-gui:guiAttribute "size=80" ,
                               "maxlength=255" ;

       knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

       knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:origName rdf:type owl:ObjectProperty ;

           rdfs:subPropertyOf knora-base:hasValue, foaf:name ;

           rdfs:label "Variantname"@de ;

           knora-base:subjectClassConstraint :entityWithNames ;

           knora-base:objectClassConstraint knora-base:TextValue ;

           salsah-gui:guiOrder "2"^^xsd:integer ;

           salsah-gui:guiElement salsah-gui:SimpleText ;

           salsah-gui:guiAttribute "size=60" ,
                                   "maxlength=255" ;

           knora-base:hasDefaultViewPermission knora-base:KnownUser ;

           knora-base:hasDefaultModifyPermission knora-base:ProjectMember .




:orgID rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasValue ;

  knora-base:subjectClassConstraint :organization ;

  knora-base:objectClassConstraint knora-base:TextValue ;

  knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:locatedAtResidence rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasLinkTo ;

  knora-base:subjectClassConstraint :entityResidence ;

  knora-base:objectClassConstraint :residence ;

  knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:locatedAtResidenceValue rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasLinkToValue ;

  knora-base:subjectClassConstraint :entityResidence ;

  knora-base:objectClassConstraint knora-base:LinkValue ;

  knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .


:residenceID rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasValue ;

  knora-base:subjectClassConstraint :residence ;

  knora-base:objectClassConstraint knora-base:TextValue ;

  knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .


:occurrence rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasValue ;

  knora-base:subjectClassConstraint :entityWithOccurrences ;

  knora-base:objectClassConstraint knora-base:TextValue ;

  knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .




:orgType rdf:type owl:ObjectProperty ;

    rdfs:subPropertyOf knora-base:hasValue ;

    knora-base:subjectClassConstraint :organization ;

    knora-base:objectClassConstraint knora-base:ListValue ;

    knora-base:hasDefaultViewPermission knora-base:KnownUser ;

    knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:hasEntityResidence rdf:type owl:ObjectProperty ;

    rdfs:subPropertyOf knora-base:hasLinkTo ;

    knora-base:subjectClassConstraint :entityWithResidences ;

    knora-base:objectClassConstraint :entityResidence ;

    knora-base:hasDefaultViewPermission knora-base:KnownUser ;

    knora-base:hasDefaultModifyPermission knora-base:ProjectMember .




:hasEntityResidenceValue rdf:type owl:ObjectProperty ;

    rdfs:subPropertyOf knora-base:hasLinkToValue ;

    knora-base:subjectClassConstraint :entityWithResidences ;

    knora-base:objectClassConstraint knora-base:LinkValue ;

    knora-base:hasDefaultViewPermission knora-base:KnownUser ;

    knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:residenceDate rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasValue ;

  knora-base:subjectClassConstraint :residence ;

  knora-base:objectClassConstraint knora-base:DateValue ;

  knora-base:hasDefaultViewPermission knora-base:KnownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:ref rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasLinkTo ;

  knora-base:subjectClassConstraint :entityWithRefs ;

  knora-base:objectClassConstraint :externalRef ;

  knora-base:hasDefaultViewPermission knora-base:KnownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .


:refValue rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasLinkToValue ;

  knora-base:subjectClassConstraint :entityWithRefs ;

  knora-base:objectClassConstraint knora-base:LinkValue ;

  knora-base:hasDefaultViewPermission knora-base:KnownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:refTitle rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasValue ;

  knora-base:subjectClassConstraint :externalRef ;

  knora-base:objectClassConstraint knora-base:TextValue ;

  knora-base:hasDefaultViewPermission knora-base:KnownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:refID rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasValue ;

  knora-base:subjectClassConstraint :externalRef ;

  knora-base:objectClassConstraint knora-base:TextValue ;

  knora-base:hasDefaultViewPermission knora-base:KnownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:refUri rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasValue ;

  knora-base:subjectClassConstraint :externalRef ;

  knora-base:objectClassConstraint knora-base:UriValue ;

  knora-base:hasDefaultViewPermission knora-base:KnownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:refPage rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasValue ;

  knora-base:subjectClassConstraint :externalRef ;

  knora-base:objectClassConstraint knora-base:TextValue ;

  knora-base:hasDefaultViewPermission knora-base:KnownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



:refType rdf:type owl:ObjectProperty ;

  rdfs:subPropertyOf knora-base:hasValue ;

  knora-base:subjectClassConstraint :externalRef ;

  knora-base:objectClassConstraint knora-base:ListValue ;

  knora-base:hasDefaultViewPermission knora-base:KnownUser ;

  knora-base:hasDefaultModifyPermission knora-base:ProjectMember .



##########################################################
#
# RESOURCES
#
##########################################################




:entityWithOccurrences rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :occurrence ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] .

:entityWithNames rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :stdName ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :origName ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] .

:entityWithResidences rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasEntityResidence ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasEntityResidenceValue ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] .


:entityWithRefs rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :ref ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refValue ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] .


:externalRef rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refType ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refID ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refUri ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refTitle ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refPage ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] .



:entityResidence rdf:type owl:Class ;

  rdfs:subClassOf :entityWithOccurrences ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty :locatedAtResidence ;
      owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty :locatedAtResidenceValue ;
      owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty :residenceDate ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] ;

    knora-base:resourceIcon "entity-residence.gif" ;

    rdfs:label "Herkunftsort der Entität"@de ;

    rdfs:comment """Herkunftsort der Entität"""@de ;

    knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

    knora-base:hasDefaultModifyPermission knora-base:ProjectMember .


#:OrgName rdf:type owl:Class ;

 # rdfs:subClassOf :entityWithOccurrences ,

#:PersName 

:residence rdf:type owl:Class ;

  rdfs:subClassOf :entityWithNames ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty :residenceID ;
      owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ;

    knora-base:resourceIcon "residence.gif" ;

    rdfs:label "Herkunftsort"@de ;

    rdfs:comment """Herkunftsort"""@de ;

    knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

    knora-base:hasDefaultModifyPermission knora-base:ProjectMember .


