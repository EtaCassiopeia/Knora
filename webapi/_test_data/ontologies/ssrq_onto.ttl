##########################################################




:entityWithOccurrences rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :occurrence ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] .

:entityWithNames rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :stdName ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :stdNameValue ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :origName ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :origNameValue ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] .
                      
:entityWithResidences rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasEntityResidence ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasEntityResidenceValue ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] .


:entityWithRefs rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :ref ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refValue ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] .


:externalRef rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refType ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refID ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refUri ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refTitle ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :refPage ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] .



:entityResidence rdf:type owl:Class ;

  rdfs:subClassOf :entityWithOccurrences ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty :locatedAtResidence ;
      owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty :locatedAtResidenceValue ;
      owl:cardinality "1"^^xsd:nonNegativeInteger
    ] ,
    [
      rdf:type owl:Restriction ;
      owl:onProperty :residenceDate ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger
    ] ;

    knora-base:resourceIcon "entity-residence.gif" ;

    rdfs:label "Herkunftsort der Entität"@de ;

    rdfs:comment """Herkunftsort der Entität"""@de ;

    knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

    knora-base:hasDefaultModifyPermission knora-base:ProjectMember .


