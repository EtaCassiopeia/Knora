:entityWithOccurrences rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :occurrence ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] .


:entityWithUnitOccurrences rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasOccurrenceUnit ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasOccurrenceUnitValue ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] .

:entityWithDates rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :date ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] .

:entityWithMentions rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasFirstMentionUnit ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasFirstMentionUnitValue ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasLastMentionUnit ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasLastMentionUnitValue ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger
                      ] .


:mentionUnit rdf:type owl:Class ;

    rdfs:subClassOf :entityWithOccurrences, 
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :mentionDate ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                      ] ;
           knora-base:resourceIcon "entity-mention.gif" ;

     rdfs:label "Erwähnungsunit einer Entität"@de ;

     rdfs:comment """Erwähnungsunit einer Entität"""@de ;

    knora-base:hasDefaultViewPermission knora-base:UnknownUser ;

    knora-base:hasDefaultModifyPermission knora-base:ProjectMember .        


:occurrenceUnit rdf:type owl:Class ;

    rdfs:subClassOf knora-base:Resource ,
                          [
                         rdf:type owl:Restriction ;
                         owl:onProperty :volume ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] ,
                          [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasOccurrenceSubUnit ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] ,
                                                [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasOccurrenceSubUnitValue ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger
                      ] .

