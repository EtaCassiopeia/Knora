##########################################################
### ###########################################
### PTMmodel:RTIparam

:RTIparam rdf:type owl:Class ;

      rdfs:subClassOf knora-base:Resource ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasGlossValue ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasGlossVersion ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasLightDir ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasImgOrientation ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasImgZoom ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasRTIshadingModel ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger                   ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasRTIdescript ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger                   ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasRTIcomment ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger                   ] ;

      knora-base:resourceIcon "RTIparam.gif" ;

      rdfs:label "RTI Parameter"@de ,
                 "RTI paramètres"@fr ,
                 "RTI parameters"@en ;

      rdfs:comment """Many RTI parameter sets can be inserted by the users and saved for future reference"""@en ,

      knora-base:hasDefaultRestrictedViewPermission knora-base:UnknownUser ;

      knora-base:hasDefaultViewPermission knora-base:KnownUser ;

      knora-base:hasDefaultModifyPermission knora-base:ProjectMember ,
                                            knora-base:Owner .


### ###########################################
### PTMmodel:RTIshadingModel

:RTIshadingModel rdf:type owl:Class ;

          rdfs:subClassOf :RTIparam ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasRTIdata ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger                   ],
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasRTIinfo ;
                         owl:minCardinality "1"^^xsd:nonNegativeInteger                   ],
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasModelDescript ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :hasModelComment ;
                         owl:minCardinality "0"^^xsd:nonNegativeInteger                   ] ;

          rdfs:label "RTI xml file"@en ;

          rdfs:comment """RTI data file with the images comprising the webPTM file (i.e. how it is tiled)"""@en ;

          knora-base:hasDefaultViewPermission knora-base:KnownUser ;

          knora-base:hasDefaultModifyPermission knora-base:ProjectMember ;

          knora-base:hasDefaultDeletePermission knora-base:Owner .



### ###########################################
### PTMmodel:PHONG

:PHONG rdf:type owl:Class ;

          rdfs:subClassOf :RTIshadingModel ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :has_kR_pvalue ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :has_kG_pvalue ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger                      ] ,
                      [
                         rdf:type owl:Restriction ;
                         owl:onProperty :has_kB_pvalue ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger                      ] ,
