{
    "salsah": {
        "ontology": "salsah",
        "link": {
            "type": "knora-base:LinkObj",
            "props": {
                "salsah:resource_reference": {
                    "predicate": [
                        "knora-base:hasLinkTo"
                    ]
                },
                "salsah:comment": {
                    "predicate": [
                        "knora-base:hasValue",
                        "knora-base:hasComment"
                    ],
                    "resource": "knora-base:TextValue"
                },
                "salsah:comment_rt": {
                    "predicate": [
                        "knora-base:hasValue",
                        "knora-base:hasComment"
                    ],
                    "resource": "knora-base:TextValue"
                }
            }
        },
        "external_res_pointer": {
            "type": "knora-base:ExternalResource",
            "props": {
                "salsah:external_id": {
                    "predicate": [
                        "knora-base:extResId"
                    ]
                },
                "salsah:external_provider": {
                    "predicate": [
                        "knora-base:extResProvider"
                    ]
                },
                "salsah:access_info": {
                    "predicate": [
                        "knora-base:extResAccessInfo"
                    ]
                }
            }
        },
        "generic_region": {
            "type": "knora-base:Region",
            "props": {
                "salsah:region_of": {
                    "predicate": [
                        "knora-base:hasLinkTo",
                        "knora-base:isRegionOf"
                    ]
                },
                "salsah:geometry": {
                    "predicate": [
                        "knora-base:hasValue",
                        "knora-base:hasGeometry"
                    ],
                    "resource": "knora-base:GeomValue"
                },
                "salsah:color": {
                    "predicate": [
                        "knora-base:hasValue",
                        "knora-base:hasColor"
                    ],
                    "resource": "knora-base:ColorValue"
                },
                "salsah:comment": {
                    "predicate": [
                        "knora-base:hasValue",
                        "knora-base:hasComment"
                    ],
                    "resource": "knora-base:TextValue"
                },
                "salsah:comment_rt": {
                    "predicate": [
                        "knora-base:hasValue",
                        "knora-base:hasComment"
                    ],
                    "resource": "knora-base:TextValue"
                }
            }
        }
    }
}